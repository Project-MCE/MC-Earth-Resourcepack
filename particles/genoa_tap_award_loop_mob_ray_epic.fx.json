        "material": "particles_blend",
        "texture": "textures/particle/tappable_particles"
      }
    },
    "components": {
      "minecraft:emitter_initialization": {
        "creation_expression": "variable.minSize = 0.15;"
      },
      "minecraft:emitter_rate_steady": {
        "spawn_rate": "Math.random(2,8)",
