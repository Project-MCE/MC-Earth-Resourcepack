{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.mud_pig": {
      "arrays": {
        "textures": {
          "Array.skins": [ "Texture.default", "Texture.muddy" ]
        },
        "geometries": {
          "Array.geos": [ "Geometry.default", "Geometry.muddy" ]
        }
      },
      "geometry": "Array.geos[query.muddy]",
