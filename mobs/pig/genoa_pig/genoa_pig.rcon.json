{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.genoa_pig": {
      "arrays": {
        "textures": {
          "Array.skins": [ "Texture.default", "Texture.saddled" ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[query.is_saddled]" ],
