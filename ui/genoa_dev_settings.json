{
  "namespace": "genoa_dev_settings",

  "genoa_dev_settings_screen@genoa_common.genoa_base_screen": {
    "$screen_content": "genoa_dev_settings.main_panel",
    "$background_scrim_visible": false,
    "screen_visibility": "opaque"
  },

  "main_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "content_stack": {
          "type": "stack_panel",
          "orientation": "vertical",
          "size": [ "100%", "100%" ],
          "controls": [
            {
              "header@genoa_common.header": {
                "$header_text": "options.genoa.dev.header",
                "$header_image": "textures/ui/header_icons/settings"
              }
            },
            {
              "pad_0": {
                "type": "panel",
                "size": [ "100%", 6 ]
              }
            },
            {
              "scrolling_panel@common.scrolling_panel": {
                "size": [ "100%", "fill" ],
                "$scrolling_content": "genoa_dev_settings.genoa_dev_settings_panel",
                "$scrolling_pane_size": [ "100%", "100%" ],
                "$show_background": false,
                "$scroll_size": [ "10px", "100%" ]
              }
            }
          ]
        }
      },
      {
        "fading_selection_info@genoa_common.fading_selection_info": {
          "$selection_info_text_offset": [ 0, -60 ]
        }
      }
    ]
  },

  "genoa_dev_settings_panel": {
    "type": "stack_panel",
    "size": [ "100% - 26px", "100%c + 50px" ], //50px for padding when scrolled to the bottom
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [ 13, 0 ],
    "controls": [
      {
        "pad_0": {
          "type": "panel",
          "size": [ "100%", 4 ]
        }
      },
      {
        "service_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.service",
          "$section_content_control": "genoa_dev_settings.genoa_dev_service_settings"
        }
      },
      { "break_0@genoa_settings.section_break": {} },
      {
        "cache_clean_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.cache",
          "$section_content_control": "genoa_dev_settings.genoa_dev_cache_cleaners"
        }
      },
      { "break_1@genoa_settings.section_break": {} },
      {
        "rendering_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.rendering",
          "$section_content_control": "genoa_dev_settings.genoa_dev_rendering_settings"
        }
      },
      { "break_2@genoa_settings.section_break": {} },
      {
        "ar_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.ar",
          "$section_content_control": "genoa_dev_settings.genoa_dev_ar_settings"
        }
      },
      { "break_2.1@genoa_settings.section_break": {} },
      {
        "server_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.server",
          "$section_content_control": "genoa_dev_settings.genoa_dev_server_settings"
        }
      },
      { "break_2.2@genoa_settings.section_break": {} },
      {
        "asa_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.asa",
          "$section_content_control": "genoa_dev_settings.genoa_dev_asa_settings"
        }
      },
      { "break_3@genoa_settings.section_break": {} },
      {
        "gps_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.map",
          "$section_content_control": "genoa_dev_settings.genoa_dev_gps_settings"
        }
      },
      { "break_4@genoa_settings.section_break": {} },
      {
        "store_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.store",
          "$section_content_control": "genoa_dev_settings.genoa_dev_store_settings"
        }
      },
      { "break_5@genoa_settings.section_break": {} },
      {
        "pack_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "Main Pack Debug Options",
          "$section_content_control": "genoa_dev_settings.genoa_dev_debug_pack_settings"
        }
      },
      { "break_5_and_a_half@genoa_settings.section_break": {} },
      {
        "persona_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.persona",
          "$section_content_control": "genoa_dev_settings.genoa_dev_persona_settings"
        }
      },
      { "break_6@genoa_settings.section_break": {} },
      {
        "content_validation_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.contentValidation",
          "$section_content_control": "genoa_dev_settings.genoa_dev_content_validation_settings"
        }
      },
      { "break_7@genoa_settings.section_break": {} },
      {
        "debug_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.debug",
          "$section_content_control": "genoa_dev_settings.genoa_dev_debug_settings"
        }
      },
      { "break_8@genoa_settings.section_break": {} },
      { "build_info@genoa_dev_settings.build_info_label_panel": {} },
      { "break_9@genoa_settings.section_break": {} },
      {
        "haptics_triggers_section@genoa_dev_settings.dev_setting_section": {
          "$section_title": "options.genoa.dev.section.hapticsTriggers",
          "$section_content_control": "genoa_dev_settings.genoa_dev_haptics_triggers"
        }
      },
      { "break_10@genoa_settings.section_break": {} },
      {
        "benchmark@genoa_settings_common.genoa_full_button_setting": {
          "$pressed_button_name": "button.benchmark_game",
          "$button_text": "options.genoa.dev.benchmarkGame"
        }
      }
    ]
  },

  "dev_setting_section": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "title": {
          "type": "label",
          "size": [ "100%", "default" ],
          "text": "$section_title",
          "font_type": "MinecraftTen",
          "override_height": 20,
          "text_alignment": "center"
        }
      },
      {
        "pad_0": {
          "type": "panel",
          "size": [ "100%", 6 ]
        }
      },
      { "section_content@$section_content_control": {} }
    ]
  },

  "genoa_dev_service_settings": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "reset_profile@genoa_settings_common.genoa_full_button_setting": {
          "$pressed_button_name": "button.reset_profile",
          "$button_text": "options.genoa.dev.resetProfile"
        }
      },
      {
        "option_genoa_enable_custom_service_environment@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "genoa_enable_custom_service_environment",
          "$setting_name": "options.genoa.dev.customEnvironment"
        }
      },
      {
        "service_environment_settings": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "bindings": [
            {
              "binding_name": "#custom_service_environment_enabled",
              "binding_type": "global",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "service_environment_dropdown@genoa_dev_settings.service_environment_dropdown": {}
            },
            {
              "option_genoa_custom_service_environment_uri@genoa_settings_common.genoa_text_edit": {
                "$show_copy_paste_buttons": true,
                "$option_label": "options.genoa.dev.customEnvironment.uri",
                "$text_box_name": "genoa_custom_service_environment_uri",
                "$option_binding_name": "#genoa_custom_service_environment_uri",
                "$max_text_edit_length": 255
              }
            }
          ]
        }
      },
      {
        "option_genoa_bypass_sign_in@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "genoa_bypass_sign_in",
          "$setting_name": "options.genoa.dev.bypass.signin"
        }
      },
      {
        "option_reset_client_id@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "dev_reset_client_id",
          "$setting_name": "options.genoa.dev.resetID"
        }
      },
      {
        "option_genoa_use_offline_service_api@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "genoa_use_offline_service_api",
          "$setting_name": "options.genoa.dev.onebox.useOfflineServiceApi"
        }
      }
    ]
  },

  "genoa_dev_cache_cleaners": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "cache_clear_collection": {
          "type": "stack_panel",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "size": [ "100%", "100%c" ],
          "orientation": "vertical",
          "collection_name": "cache_locations",
          "factory": {
            "name": "cache_clear_collection_factory",
            "control_name": "genoa_settings.clear_cache_entry"
          },
          "bindings": [
            {
              "binding_name": "#cache_locations_size",
              "binding_type": "global",
              "binding_name_override": "#collection_length"
            }
          ]
        }
      },
      {
        "option_clear_store_cache_button@genoa_settings_common.genoa_full_button_setting": {
          "$button_text": "options.genoa.dev.storeCache",
          "$pressed_button_name": "dev_clear_store_cache_button"
        }
      },
      {
        "option_clear_all_cache_button@genoa_settings_common.genoa_full_button_setting": {
          "$button_text": "options.genoa.dev.storeCache.all",
          "$pressed_button_name": "dev_clear_all_cache_button"
        }
      }
    ]
  },

  "genoa_dev_rendering_settings": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "option_genoa_post_fx@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "genoa_post_fx_enabled",
          "$setting_name": "options.genoa.dev.postFX"
        }
      },
      {
        "option_genoa_use_font_overrides@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "genoa_use_font_overrides",
          "$setting_name": "options.genoa.dev.ttfOverrides"
        }
      }
    ]
  },

  "genoa_dev_content_validation_settings": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "validate_item_catalog@genoa_settings_common.genoa_full_button_setting": {
          "$pressed_button_name": "button.validate_item_catalog",
          "$button_text": "options.genoa.dev.validateItemCatalog"
        }
      },
      {
        "validate_item_catalog_blocks@genoa_settings_common.genoa_full_button_setting": {
          "$pressed_button_name": "button.validate_item_catalog_blocks",
          "$button_text": "options.genoa.dev.validateItemCatalogBlocks"
        }
      }
    ]
  },

  "treatment_action_button_content": {
    "type": "image",
    "size": [ "100% - 8px", "100% - 8px" ],
    "$treatment_action_image_bind": "#treatment_action_image",
    "texture": "$treatment_action_image"
  },

  "genoa_dev_treatment_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", 32 ],
    "controls": [
      {
        "treatment_label_panel": {
          "type": "image",
          "size": [ "fill", "100%" ],
          "texture": "textures/ui/box_simple",
          "controls": [
            {
              "treatment_label": {
                "type": "label",
                "size": [ "100% - 12px", 20 ],
                "override_height": 20,
                "text_alignment": "left",
                "$treatment_label_binding": "#treatment_id",
                "text": "$treatment_label_binding",
                "bindings": [
                  {
                    "binding_type": "collection",
                    "binding_collection_name": "$treatment_collection_name",
                    "binding_name": "$treatment_label_binding"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "treatment_action_button@genoa_common.button": {
          "size": [ "100%y", "100%" ],
          "$button_content": "genoa_dev_settings.treatment_action_button_content",
          "$pressed_button_name": "$treatment_action_button_name",
          "$bevel_texture": "textures/ui/tile_bevel_dark",
          "$shadow_enabled": false,
          "$background_style": "color",
          "$background_color": "$genoa_nav_bar_background_color",
          "$glow_enabled": false,
          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "$treatment_collection_name",
              "binding_condition": "visible"
            }
          ]
        }
      }
    ]
  },

  "treatment_grid_section": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "bindings": [
      {
        "binding_name": "$treatment_grid_section_visible",
        "binding_name_override": "#visible"
      }
    ],
    "controls": [
      {
        "treatments_label_panel": {
          "type": "image",
          "size": [ "100%", 18 ],
          "texture": "textures/ui/Banners",
          "controls": [
            {
              "treatments_label": {
                "type": "label",
                "offset": [ 3, 0 ],
                "text": "$treatment_grid_section_label",
                "text_alignment": "left",
                "color": "$body_text_color",
                "layer": 1
              }
            }
          ]
        }
      },
      {
        "treatment_grid": {
          "type": "grid",
          "size": [ "100%", "100%c" ],
          "grid_item_template": "genoa_dev_settings.genoa_dev_treatment_content",
          "grid_dimension_binding": "$treatment_grid_dimension_binding",
          "collection_name": "$treatment_collection_name",
          "bindings": [
            {
              "binding_name": "$treatment_grid_dimension_binding"
            }
          ]
        }
      }
    ]
  },

  "treatment_display": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "spacer_0": {
          "type": "panel",
          "size": [ "100%", 4 ]
        }
      },
      {
        "override_treatment_options_panel": {
          "type": "stack_panel",
          "orientation": "vertical",
          "size": [ "100%", "100%c" ],
          "bindings": [
            {
              "binding_name": "#override_treatment_options_visible",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "add_treatments_panel": {
                "type": "stack_panel",
                "size": [ "100%", "100%cm" ],
                "orientation": "horizontal",
                "controls": [
                  {
                    "text_edit_treatment@genoa_settings_common.genoa_text_edit": {
                      "size": [ "fill", "100%c" ],
                      "$option_label": "options.dev_addTreatmentId",
                      "$text_box_name": "dev_treatment_id",
                      "$option_binding_name": "#dev_treatment_id",
                      "$option_enabled_binding_name": "#dev_treatment_id_enabled",
                      "$show_copy_paste_buttons": true,
                      "$max_text_edit_length": 50
                    }
                  },
                  {
                    "add_button_panel": {
                      "type": "panel",
                      "size": [ 40, 40 ],
                      "controls": [
                        {
                          "add_button@genoa_settings_common.genoa_button_setting": {
                            "$pressed_button_name": "button.add_treatment_by_name",
                            "size": [ "100%", 30 ],
                            "offset": [ 0, 4 ],
                            "anchor_from": "bottom_right",
                            "anchor_to": "bottom_right",
                            "$button_text": "options.dev_addLabel"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "in_use_treatment_grid_section@genoa_dev_settings.treatment_grid_section": {
          "$treatment_grid_section_label": "options.dev_currentTreatmentsTitle",
          "$treatment_grid_section_visible": "#treatment_grid_section_visible",
          "$treatment_grid_dimension_binding": "#treatments_grid_dimension",
          "$treatment_collection_name": "treatment_collection",
          "$treatment_action_button_name": "button.remove_treatment",
          "$treatment_action_image": "textures/ui/red_x"
        }
      },
      {
        "spacer_1": {
          "type": "panel",
          "size": [ "100%", 4 ]
        }
      },
      {
        "treatment_cache_management_panel": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", 30 ],
          "controls": [
            {
              "reset_to_default@genoa_settings_common.genoa_button_setting": {
                "size": [ "fill", "100%" ],
                "$pressed_button_name": "button.reset_to_default_treatments",
                "$button_text": "Default"
              }
            },
            {
              "center_padding": {
                "type": "panel",
                "size": [ 10, "100%" ]
              }
            },
            {
              "clear_treatments@genoa_settings_common.genoa_button_setting": {
                "size": [ "fill", "100%" ],
                "$pressed_button_name": "button.clear_treatment_list",
                "$button_text": "Clear All"
              }
            }
          ]
        }
      },
      {
        "spacer_2": {
          "type": "panel",
          "size": [ "100%", 2 ]
        }
      }
    ]
  },

  "genoa_dev_store_treatment_options": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "display_treatments_toggle@genoa_settings_common.genoa_setting_main": {
          "$setting_type": "bool",
          "$bool_option_id": "dev_display_treatments",
          "$setting_name": "Display Treatments"
        }
      },
      {
        "display_panel": {
          "type": "image",
          "size": [ "100%", "100%c + 24px" ],
          "texture": "textures/ui/White",
          "color": "$9_color_format",
          "alpha": 0.4,
          "keep_ratio": false,
          "bindings": [
            {
              "binding_name": "#treatment_section_visible",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "indent_panel": {
                "type": "stack_panel",
                "orientation": "vertical",
                "size": [ "100% - 24px", "100%c" ],
                "controls": [
                  {
                    "override_treatments_toggle@genoa_settings_common.genoa_setting_main": {
                      "$setting_type": "bool",
                      "$bool_option_id": "dev_override_treatments",
                      "$setting_name": "Override Treatments"
                    }
                  },
                  {
                    "treatment_display@genoa_dev_settings.treatment_display": {}
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "dropdown_label": {
    "type": "label",
    "$dropdown_text_binding": "('#' + $dropdown_name + '_dropdown_label')",
    "text": "$dropdown_text_binding",
    "color": "$genoa_header_text_color",
    "font_type": "MinecraftTen",
    "override_height": 20,
    "bindings": [
      {
        "binding_name": "$dropdown_text_binding"
      }
    ]
  },

  "dropdown_option_content": {
    "type": "panel",
    "size": [ "100%", 40 ],
    "controls": [
      {
        "dropdown_option_button@genoa_common.button": {
          "$background_style": "color",
          "$background_color": "$genoa_dark_button_color",

          "$pressed_button_name": "('button.' + $dropdown_name + '_option')",
          "$button_text": "('#' + $dropdown_name + '_option_text_binding')",
          "$button_content": "genoa_common.button_content_text_bindable",
          "$button_text_collection_name": "$dropdown_collection_name",
          "$button_font_height": 20,
          "$button_text_binding_type": "collection",

          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "$dropdown_collection_name"
            }
          ]
        }
      },
      {
        "active_image@genoa_dev_settings.service_environment_supported_image": {
          "offset": [ 6, 0 ],
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "layer": 25,

          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "$dropdown_collection_name"
            },
            {
              "binding_type": "collection",
              "binding_collection_name": "$dropdown_collection_name",
              "binding_name": "('#is_' + $dropdown_name + '_option_active')",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  "dropdown_grid": {
    "type": "grid",
    "size": [ "100%", "100%c" ],
    "grid_item_template": "genoa_dev_settings.dropdown_option_content",
    "$dropdown_grid_dimension_binding|default": "('#' + $dropdown_name + '_grid_dimension_binding')",
    "grid_dimension_binding": "$dropdown_grid_dimension_binding",
    "$dropdown_collection_name|default": "($dropdown_name + '_collection')",
    "collection_name": "$dropdown_collection_name",

    "bindings": [
      {
        "binding_name": "$dropdown_grid_dimension_binding"
      }
    ]
  },

  "override_date_panel": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "100%cm" ],
    "$one_line_layout": false,
    "$text_edit_box_text_type": "NumberChars",
    "$option_text_edit_control_size": [ 30, 30 ],
    "$option_generic_panel_size": [ 30, "100%c" ],
    "$option_label_alignment": "center",
    "controls": [
      {
        "year_text_box@genoa_settings_common.genoa_text_edit": {
          "size": "$option_generic_panel_size",
          "$option_label": "Year",
          "$text_box_name": "dev_date_year_override_text_box",
          "$option_binding_name": "#dev_date_year_override",
          "$max_text_edit_length": 4
        }
      },
      {
        "year_month_slash": {
          "type": "label",
          "size": [ 16, "default" ],
          "text": " / ",
          "text_alignment": "center",
          "color": "$main_header_text_color"
        }
      },
      {
        "month_text_box@genoa_settings_common.genoa_text_edit": {
          "size": "$option_generic_panel_size",
          "$option_label": "Month",
          "$text_box_name": "dev_date_month_override_text_box",
          "$option_binding_name": "#dev_date_month_override",
          "$max_text_edit_length": 2
        }
      },
