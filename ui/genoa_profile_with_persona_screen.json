// Temporary profile screen which is created only when persona is enabled, and features the "Appearance" tab
// Once persona is fully integrated, this screen should be merged with genoa_profile_screen.json 
// and the condition in GenoaSceneFactory::createProfileScreen() should be removed.
{
  "namespace": "genoa_profile_persona",

  // SCREEN DEFINITION
  "profile_persona_screen@genoa_common.genoa_base_screen": {
    "$screen_content": "genoa_profile_persona.main_content",
    "screen_visibility": "opaque",
    "$use_background": true,
    "$screen_background_texture": "textures/ui/background_blurred",
    "$background_scrim_visible": false
  },

  // CONTENT PANEL
  "main_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$activity_log_collection_name": "#activity_log_list",
    "$overview_bindings": [
      {
        "binding_name": "#overview_visible",
        "binding_name_override": "#visible"
      }
    ],
    "$activity_log_bindings": [
      {
        "binding_name": "#activity_log_visible",
        "binding_name_override": "#visible"
      }
    ],
    "$appearance_bindings": [
      {
        "binding_name": "#appearance_visible",
        "binding_name_override": "#visible"
      }
    ],
    "$settings_bindings": [
      {
        "binding_name": "#settings_visible",
        "binding_name_override": "#visible"
      }
    ],
    "controls": [
      {
        "deep_linking_progress_modal@genoa_persona.loading_in_progress_modal": {
          "$loading_in_progress_binding": "#deep_linking_in_progress",
          "$loading_text": "profile.modal.label.loadingPersona"
        }
      },
      {
        "screen_input_panel": {
          "type": "input_panel",
          "size": [ "100%", "100%" ],
          "button_mappings": [
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "button.disable_tool_tip_screen_fill",
              "mapping_type": "pressed"
            }
          ]
        }
      },
      { "settings_full_screen_controls@genoa_settings.settings_full_screen_controls": {} },
      {
        "main_stack": {
          "type": "stack_panel",
          "size": [ "100%", "100%" ],
          "use_new_layout": true,
          "controls": [
            {
              // HEADER
              "header_section@genoa_common.header": {
                "$header_text": "profile.header",
                "$header_icon_control": "genoa_common.profile_head",
                "size": [ "100%", 68 ]
              }
              // END OF HEADER
            },
            {
              "pad_0": {
                "type": "panel",
                "size": [ "100%", 23 ]
              }
            },
            {
              "tab_selection_panel@genoa_profile_persona.tab_selection_panel": {
                "$tab_button_controls": [
                  {
                    "overview_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.overview_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/all",
                      "$tab_button_visible_binding_name": "#overview_visible"
                    }
                  },
                  {
                    "appearance_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.appearance_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/8x8/persona",
                      "$tab_button_visible_binding_name": "#appearance_visible",
                      "$tab_content": "genoa_profile_persona.appearance_button_notification_icon_image",
                      "$alpha_binding_name": "#appearance_alpha",
                      "$enabled_binding_name": "#appearance_enabled"
                    }
                  },
                  {
                    "activity_log_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.activity_log_tab",
                      "$tab_button_texture": "textures/ui/profile/activity_log",
                      "$tab_button_visible_binding_name": "#activity_log_visible",
                      "$alpha_binding_name": "#activity_log_alpha",
                      "$enabled_binding_name": "#activity_log_enabled"
                    }
                  },
                  {
                    "settings_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.settings_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/9x8/settings",
                      "$tab_button_visible_binding_name": "#settings_visible"
                    }
                  }
                ],
                "bindings": [
                  {
                    "binding_name": "(not #enable_level_up_rewards)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "tab_selection_panel_with_level_up_rewards@genoa_profile_persona.tab_selection_panel": {
                "$tab_button_controls": [
                  {
                    "overview_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.overview_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/all",
                      "$tab_button_visible_binding_name": "#overview_visible"
                    }
                  },
                  {
                    "appearance_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.appearance_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/8x8/persona",
                      "$tab_button_visible_binding_name": "#appearance_visible",
                      "$tab_content": "genoa_profile_persona.appearance_button_notification_icon_image",
                      "$alpha_binding_name": "#appearance_alpha",
                      "$enabled_binding_name": "#appearance_enabled"
                    }
                  },
                  {
                    "level_up_rewards_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.level_up_rewards_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/7x8/crown",
                      "$tab_button_visible_binding_name": "#level_up_rewards_visible",
                      "$tab_content": "genoa_profile_persona.level_up_rewards_button_notification_icon_image",
                      "$alpha_binding_name": "#level_up_rewards_alpha",
                      "$enabled_binding_name": "#level_up_rewards_enabled"
                    }
                  },
                  {
                    "activity_log_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.activity_log_tab",
                      "$tab_button_texture": "textures/ui/profile/activity_log",
                      "$tab_button_visible_binding_name": "#activity_log_visible",
                      "$alpha_binding_name": "#activity_log_alpha",
                      "$enabled_binding_name": "#activity_log_enabled"
                    }
                  },
                  {
                    "settings_button@genoa_common.tab_button": {
                      "$pressed_button_name": "button.settings_tab",
                      "$tab_button_texture": "textures/ui/icons/pixel_icons/9x8/settings",
                      "$tab_button_visible_binding_name": "#settings_visible"
                    }
                  }
                ],
                "bindings": [
                  {
                    "binding_name": "#enable_level_up_rewards",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "xp_bar@genoa_profile_persona.xp_bar": {
                "bindings": [
                  {
                    "binding_name": "#level_up_rewards_visible",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            { "tab_layout@genoa_profile_persona.tab_layouts": {} }
          ]
        }
      }
    ]
  },

  "tab_layouts": {
    "type": "stack_panel",
    "orientation": "vertical",
    "use_new_layout": true,
    "size": [ "100%", "fill" ],
    "controls": [
      {
        "non_fullscreen_tabs": {
          "type": "stack_panel",
          "orientation": "vertical",
          "use_new_layout": true,
          "size": [ "100%", "100%" ],
          "bindings": [
            {
              "binding_name": "(not #settings_visible)",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "pad_0": {
                "type": "panel",
                "size": [ "100%", 8 ]
              }
            },
            {
              "tab_specific_layout_panel@genoa_profile_persona.tab_specific_layout_panel": {
                "size": [ "100%", "fill" ]
              }
            }
          ]
        }
      },
      {
        "pad_0": {
          "type": "panel",
          "size": [ "100%", 8 ]
        }
      },
      {
        "fade_bars@genoa_common.fading_scroll_mask_bars": {
          "layer": 20,
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "$source_control_name_override": "settings_scroll_view"
        }
      },
      {
        "settings_layout_panel@common.scrolling_panel": {
          "size": [ "100%", "fill" ],
          "$scrolling_content": "genoa_settings.genoa_settings_panel",
          "$scrolling_pane_size": [ "100%", "100%" ],
          "$show_background": false,
          "$scroll_size": [ "10px", "100%" ],
          "bindings": "$settings_bindings",
          "$scroll_view_name": "settings_scroll_view",
          "offset": [ 0, -3 ]
        }
      }
    ]
  },

  "appearance_button_notification_icon_image@genoa_persona.status_image": {
    "size": [ 10, 10 ],
    "offset": [ "100%x", -10 ],
    "texture": "textures/ui/profile_notification_icon",
    "$status_image_anchor": "center",
    "$status_image_visible_binding_type": "global",
    "$status_image_visible_binding_name": "#are_new_persona_offers_available"
  },

  "level_up_rewards_button_notification_icon_image@genoa_persona.status_image": {
    "size": [ 10, 10 ],
    "offset": [ "100%x", -10 ],
    "texture": "textures/ui/profile_notification_icon",
    "$status_image_anchor": "center",
    "$status_image_visible_binding_type": "global",
    "$status_image_visible_binding_name": "#new_level_up_rewards_available"
  },

  "tab_selection_panel@genoa_common.tab_selection_panel": {
    "$text": "#profile_tab_filter_name",
    "$text_binding": [
      {
        "binding_name": "$text",
        "binding_condition": "always"
      }
    ]
  },

  "gamer_tag@genoa_common.boxed_icon_label": {
    "anchor_to": "top_middle",
    "anchor_from": "top_middle",
    "$label_text_height": 22,
    "$label_text": "#player_name",
    "$label_text_max_size": [ 320, 0 ],
    "$text_bindings": [
      {
        "binding_name": "$label_text",
        "binding_condition": "once"
      }
    ],
    "bindings": "$overview_bindings"
  },

  // SKIN VIEWER PANEL
  "skin_view": {
    "type": "panel",
    "controls": [
      {
        "vignette": {
          "type": "custom",
          "size": [ "100%", "100%" ],
          "renderer": "vignette",
          "vignette_scene": "genoa:profile_vignette",
          "property_bag": {
            "vignette_skins": "$vignette_skins"
          },
          "bindings": [
            {
              "binding_type": "view",
              "source_property_name": "#vignette_id",
              "target_property_name": "#vignette_id",
              "invoke_screen_controller": true
            },
            {
              "binding_name": "#vignette_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "preview@genoa_profile_persona.skin_viewer_panel": {
        }
      },
      {
        "skin_view_panel_appearance": {
          "type": "panel",
          "bindings": "$appearance_bindings",
          "controls": [
            {
              "retry_button@genoa_store.button_with_icon": {
                "size": [ 80, 80 ],
                "layer": 2,
                "$button_icon_texture": "textures/ui/refresh",
                "$button_icon_size": [ 36, 36 ],
                "$pressed_button_name": "button.retry_appearance_preset",
                "bindings": [
                  {
                    "binding_name": "#is_retry_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            },
            {
              "progress_loading@genoa_common.progress_loading": {
                "bindings": [
                  {
                    "binding_name": "#is_current_preset_loading_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "skin_viewer_panel": {
    "type": "input_panel",
    "size": [ "100%", "100%" ],
    "gesture_tracking_button": "button.disable_tool_tip_screen_fill",
    "always_handle_controller_direction": true,
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.disable_tool_tip_screen_fill",
        "mapping_type": "pressed"
      }
    ],
    "property_bag": {
      "#rotation_speed": 2.0
    },
    "bindings": [
      {
        "binding_type": "global",
        "binding_name": "#gesture_info",
        "binding_condition": "always_when_visible"
      },
      {
        "binding_type": "view",
        "source_property_name": "#gesture_delta_source",
        "target_property_name": "#gesture_delta_source"
      },
      {
        "binding_type": "view",
        "source_property_name": "#gesture_mouse_delta_x",
        "target_property_name": "#gesture_mouse_delta_x"
      }
    ]
  },
  // END OF SKIN VIEWER PANEL

  "tab_specific_layout_panel": {
    "type": "panel",
    "controls": [
      {
        "overview_layout_panel@genoa_profile_persona.overview_layout_panel": {
          "size": [ "100%", "100%" ],
          "bindings": "$overview_bindings"
        }
      },
      {
        "activity_log_layout_panel@genoa_profile_persona.activity_log_layout_panel": {
          "size": [ "100%", "100%" ],
          "bindings": "$activity_log_bindings"
        }
      },
      {
        "appearance_layout_panel@genoa_profile_persona.appearance_layout_panel": {
          "size": [ "100%", "100%" ],
          "bindings": "$appearance_bindings"
        }
      },
      {
        "level_up_rewards_layout_panel@genoa_profile_persona.level_up_rewards_layout_panel": {
          "bindings": [
            {
              "binding_name": "#level_up_rewards_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  // OVERVIEW TAB LAYOUT
  "overview_layout_panel": {
    "type": "stack_panel",
    "use_new_layout": true,
    "controls": [
      {
        "gamer_tag@genoa_profile_persona.gamer_tag": {}
      },
      {
        "pad_0": {
          "type": "panel",
          "size": [ "100%", 2 ]
        }
      },
      {
        "persona_and_stats_panel": {
          "size": [ "100%", "fill" ],
          "type": "panel",
          "controls": [
            {
              "skin_view@genoa_profile_persona.skin_view": {
                "size": [ "100%", "55%" ],
                "anchor_from": "top_middle",
                "anchor_to": "top_middle"
              }
            },
            {
              "overview_scroll_panel": {
                "size": [ "100%", "45%" ],
                "type": "panel",
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "controls": [
                  {
                    "tooltip": {
                      "type": "panel",
                      "anchor_from": "top_middle",
                      "anchor_to": "bottom_middle",
                      "offset": [ 0, -10 ],
                      "size": [ 330, 85 ],
                      "controls": [
                        {
                          "tooltip@genoa_profile_persona.new_tool_tip": {
                            "layer": 12,
                            "bindings": [
                              {
                                "binding_name": "#stat_tooltip_visible",
                                "binding_name_override": "#visible"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "fade_bars@genoa_common.fading_scroll_mask_bars": {
                      "layer": 20,
                      "anchor_from": "top_middle",
                      "anchor_to": "top_middle",
                      "$source_control_name_override": "overview_persona_scroll_view"
                    }
                  },
                  {
                    "scrolling_panel@common.scrolling_panel": {
                      "size": [ "100%", "100% - 5px" ],
                      "anchor_from": "bottom_middle",
                      "anchor_to": "bottom_middle",
                      "$show_background": false,
                      "$scroll_fade_distance": 4.0,
                      "$scrolling_content": "genoa_profile_persona.overview_scrolling_content",
                      "$scroll_box_touch_image_control": "genoa_common.scroll_bar_control",
                      "$scroll_view_name": "overview_persona_scroll_view"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "xp_bar": {
    "type": "panel",
    "size": [ "100%", 60 ],
    "controls": [
      {
        "xp_label": {
          "type": "label",
          "offset": [ 12, 13 ],
          "font_type": "MinecraftSeven",
          "override_height": 18,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "text": "profile.level"
        }
      },
      {
        "xp_tally": {
          "type": "label",
          "offset": [ -13, 13 ],
          "font_type": "MinecraftSeven",
          "override_height": 18,
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "text": "#xp_tally",
          "bindings": [
            {
              "binding_name": "#xp_tally",
              "binding_condition": "always"
            }
          ]
        }
      },
      {
        "bar_box": {
          "type": "image",
          "size": [ 350, 20 ],
          "offset": [ 0, -2 ],
          "texture": "textures/ui/9_slice/box_simple",
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "controls": [
            {
              "xp_bar_color@genoa_common.white": {
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "layer": 2,
                "offset": [ 5, 0 ],
                "size": [ 340, 10 ],
                "color": "$genoa_hud_experience_bar_color",
                "clip_direction": "left",
                "clip_pixelperfect": false,
                "controls": [
                  {
                    "xp_bar_gradient": {
                      "type": "image",
                      "texture": "textures/ui/bar_bevel_basic",
                      "clip_direction": "left",
                      "clip_pixelperfect": false,
                      "bindings": [
                        {
                          "binding_name": "#xp_bar_size",
                          "binding_name_override": "#clip_ratio",
                          "binding_condition": "always"
                        }
                      ]
                    }
                  }
                ],
                "bindings": [
                  {
                    "binding_name": "#xp_bar_size",
                    "binding_name_override": "#clip_ratio",
                    "binding_condition": "always"
                  }
                ]
              }
            },
            {
              "xp_overlay_grid": {
                "type": "grid",
                "layer": 3,
                "size": [ "100% - 5px", 10 ],
                "offset": [ 5, 0 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "grid_item_template": "genoa_profile_persona.xp_overlay_grid_item",
                "$grid_name": "#xp_overlay",
                "collection_name": "$grid_name",
                "grid_dimension_binding": "$grid_name",
                "bindings": [
                  {
                    "binding_name": "$grid_name"
                  }
                ]
              }
            },
            {
              "player_level@genoa_profile_persona.player_level_display": {
                "layer": 6,
                "$offset_size": "large",
                "offset": [ 0, -24 ]
              }
            }
          ]
        }
      }
    ]
  },

  "overview_scrolling_content": {
    "type": "stack_panel",
    "use_new_layout": true,
    "controls": [
      {
        "xp_bar@genoa_profile_persona.xp_bar": {}
      },
      {
        "pad_stats": {
          "type": "panel",
          "size": [ "100%", 10 ]
        }
      },
      {
        "stats_row_0@genoa_profile_persona.stats_row": {
          "$stat_name_1": "blocks_placed",
          "$stat_name_2": "blocks_collected",
          "$stat_name_3": "mobs_collected",
          "$stat_name_4": "time_played"
        }
      },

      //the extra stats below are under a feature flag controlled by the #enable_player_stats binding
      {
        "player_stats": {
          "type": "stack_panel",
          "use_new_layout": true,
          "size": [ "100%", "100%c" ],
          "bindings": [
            {
              "binding_name": "#enable_player_stats",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "pad_stats_row_1": {
                "type": "panel",
                "size": [ "100%", 5 ]
              }
            },
            {
              "stats_row_1@genoa_profile_persona.stats_row": {
                "$stat_name_1": "challenges_completed",
                "$stat_name_2": "tools_broken",
                "$stat_name_3": "items_crafted",
                "$stat_name_4": "items_smelted"
              }
            },
            {
              "pad_stats_row_2": {
                "type": "panel",
                "size": [ "100%", 5 ]
              }
            },
            {
              "stats_row_2@genoa_profile_persona.stats_row": {
                "$stat_name_1": "adventures_played",
                "$stat_name_2": "tappables_collected",
                "$stat_name_3": "number_of_deaths",
                "$stat_name_4": "mobs_killed"
              }
            },
            {
              "pad_stats_row_3": {
                "type": "panel",
                "size": [ "100%", 5 ]
              }
            },
            {
              "stats_row_3@genoa_profile_persona.stats_row": {
                "$stat_name_1": "shared_buildplate_views",
                "$stat_2_ignored": true,
                "$stat_3_ignored": true,
                "$stat_4_ignored": true
              }
            }
          ]
        }
      },
      {
        "pad_end": {
          "type": "panel",
          "size": [ "100%", 35 ]
        }
      }
    ]
  },
  // END OVERVIEW TAB LAYOUT

  // LEVEL UP REWARDS TAB LAYOUT
  "empty_reward": {
    "type": "panel",
    "size": [ 60, 60 ],
    "controls": [
      {
        "reward_grid_background@genoa_common.white": {
          "texture": "textures/ui/crafting_smelting_recipe_grid_none_bg",
          "controls": [
            {
              "slot_bevel@genoa_common.white": {
                "texture": "textures/ui/slot_bevel_simple"
              }
            }
          ]
        }
      }
    ]
  },

  "reward": {
    "type": "panel",
    "size": [ 60, 60 ],
    "controls": [
      {
        "reward_box@genoa_challenges.reward": {
          "size": [ "100%", "100%" ], // necessary, the default size fo reward in genoa_challenges is set to 50, 50
          "$reward_alpha": "#collected_level_reward_alpha",
          "$reward_quantity_propagate_alpha": true,
          "$reward_quantity_alpha_bindings": [
            {
              "binding_name": "$reward_alpha",
              "binding_name_override": "#alpha"
            },
            {
              "binding_type": "collection_details"
            }
          ],
          "$reward_image_bindings": [
            {
              "binding_name": "$reward_alpha",
              "binding_name_override": "#alpha"
            },
            {
              "binding_name": "#level_up_reward_item_texture",
              "binding_name_override": "#texture"
            },
            {
              "binding_name": "#level_up_reward_item_texture_file_system",
              "binding_name_override": "#texture_file_system"
            },
            {
              "binding_type": "collection_details"
            }
          ],
          "$reward_quantity": "#level_up_reward_quantity",
          "$reward_quantity_bindings": [
            {
              "binding_name": "$reward_quantity"
            },
            {
              "binding_type": "collection_details"
            }
          ],
          "$button_mappings": [
            {
              "from_button_id": "button.menu_select",
