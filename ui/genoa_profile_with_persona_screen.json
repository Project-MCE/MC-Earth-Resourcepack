          "clip_state_change_event": "#persona_list_clipping_event",

          "$persona_asset_prefix|default": "asset",
          "$persona_asset_binding_type|default": "collection",
          "$persona_asset_collection_details|default": "collection_details",
          "$persona_asset_collection_name|default": "asset_collection",
          "collection_name": "$persona_asset_collection_name",
          "bindings": [
            {
              "binding_name": "#persona_grid_slots",
              "binding_name_override": "#maximum_grid_items"
            }
          ]
        }
      }
    ]
  },

  "persona_button_panel": {
    "type": "panel",
    "size": "$asset_offer_size",

    "controls": [
      {
        "persona_offer_button@genoa_common.button": {
          "size": [ "100%", "100%" ],
          "$background_keep_ratio": false,
          "$bevel_texture": "textures/ui/slot_bevel_simple",
          "$button_content": "genoa_profile_persona.selectable_persona",
          "$locked_control": "genoa_persona.no_offer",
          "$shadow_enabled": false,
          "$background_style": "image",
          "$button_background_image_texture": "textures/ui/inventory_slot_background",
          "$glow_enabled": false,
          "sound_name": "item.select",
          "bindings": [
            {
              "binding_type": "$persona_asset_collection_details",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_collection_prefix": "$persona_asset_prefix"
            },
            {
              "binding_type": "$persona_asset_binding_type",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_name": "#button_enabled",
              "binding_name_override": "#enabled"
            }
          ]
        }
      }
    ]
  },

  "selectable_persona@genoa_persona.selectable_grid_offer": {
    "$selectable_content": "genoa_profile_persona.persona_button_content",
    "$selected_size|default": [ 121, 152 ],
    "$selection_collection_name": "$persona_asset_collection_name",
    "$selection_collection_prefix": "$persona_asset_prefix",
    "$selection_border_binding": "#persona_selection_visible",
    "$selected_offset": [ 0, -8 ]
  },

  "persona_button_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$asset_image_size|default": [ 51, 101 ],

    "controls": [
      {
        "existing_persona": {
          "type": "custom",
          "$paperdoll_size|default": [ 80, 125 ],
          "size": "$paperdoll_size",
          "layer": 2,
          "renderer": "paper_doll_renderer",
          "use_selected_skin": false,
          "use_skin_gui_scale": true,
          "$paperdoll_offset|default": [ 0, -18 ],
          "offset": "$paperdoll_offset",
          "bindings": [
            {
              "binding_type": "$persona_asset_binding_type",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_name": "#is_preset_visible",
              "binding_name_override": "#visible",
              "binding_condition": "always"
            },
            {
              "binding_type": "$persona_asset_binding_type",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_name": "#persona_skin"
            }
          ]
        }
      },
      {
        "new_persona_image": {
          "type": "image",
          "size": "$asset_image_size",
          "texture": "textures/ui/profile/create_new_persona",
          "bindings": [
            {
              "binding_type": "$persona_asset_binding_type",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_name": "#is_preset_add_visible",
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ]
        }
      },
      {
        "progress_loading@genoa_common.progress_loading": {
          "bindings": [
            {
              "binding_type": "$persona_asset_binding_type",
              "binding_collection_name": "$persona_asset_collection_name",
              "binding_name": "#is_preset_loading_visible",
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ]
        }
      }
    ]
  }
  // END PERSONA CONTENT
}
